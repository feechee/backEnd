<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio</th>
      <th scope="col">Foto</th>
    </tr>
  </thead>
  <tbody>
      {{#each productos}}
    <tr>
      <td>{{title}}</td>
      <td>{{price}}</td>
      <td>{{thumbnail}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<a class="btn btn-primary" href="/" role="button">Ingresar Productos</a>